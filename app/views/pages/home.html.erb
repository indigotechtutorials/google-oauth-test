<div>
  <h1 class="font-bold text-4xl">Hello user you have signed in from Google </h1>
  <p><%= Current.user.first_name %> <%= Current.user.last_name %> </p>
  <%= image_tag(Current.user.avatar, class: "w-16 h-16 object-cover") if Current.user.avatar.attached? %>
  <%= link_to "Sign out", session_path, data: { turbo_method: :delete } %>
</div>
